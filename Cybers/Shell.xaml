<Window x:Class="Cybers.Shell"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:prism="http://www.codeplex.com/prism"
        xmlns:inf="clr-namespace:Cybers.Infrustructure;assembly=Cybers.Infrustructure"
        xmlns:local="clr-namespace:Cybers"
        mc:Ignorable="d"
        Height="600" Width="700"
        Title="Shell" d:DesignWidth="300" d:DesignHeight="300" BorderBrush="DarkBlue" BorderThickness="2">
        <ContentControl prism:RegionManager.RegionName="{x:Static inf:RegionNames.MainContentRegion}"/>
</Window>
