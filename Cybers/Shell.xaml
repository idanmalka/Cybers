<Window x:Class="Cybers.Shell"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:prism="http://www.codeplex.com/prism"
        xmlns:inf="clr-namespace:Cybers.Infrustructure;assembly=Cybers.Infrustructure"
        xmlns:local="clr-namespace:Cybers"
        mc:Ignorable="d"
        Title="Shell">
    <DockPanel LastChildFill="True">
        <ContentControl DockPanel.Dock="Bottom" prism:RegionManager.RegionName="{x:Static inf:RegionNames.BottomToolbarReegion}" Margin="5" />
        <ContentControl prism:RegionManager.RegionName="{x:Static inf:RegionNames.MainContentRegion}"/>
    </DockPanel>
</Window>
